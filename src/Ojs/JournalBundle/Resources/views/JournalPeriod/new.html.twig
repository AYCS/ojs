{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.journal_period_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('ojs_journal_dashboard_index', {journalId: selectedJournal().id}), 'title': 'dashboard'|trans},
    {'link': path('ojs_journal_period_index', {'journalId': selectedJournal().id}),'title': 'journal.period'|trans},
    {'title': 'title.journal_period_new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ 'title.journal_period_new'|trans }}</h1>

    {{ form(form) }}

    {{
    actions({
        'back': {'href': path('ojs_journal_period_index', {'journalId': selectedJournal().id})},
    })
    }}
{% endblock %}
