{% extends '::ojsbase.html.twig' %}
{% set hideleft = true %}
{% block title %}{{ 'title.institution_theme_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
        {'link': path('ojs_user_index'), 'title': 'user_page'|trans},
        {'link': path('ojs_institution_manager_theme_index', {institutionId: institution.id}),'title': 'title.institution_theme'|trans},
        {'title': 'title.institution_theme_new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ "institutiontheme.new"|trans }}</h1>

    <a href="{{ path('ojs_institution_manager_theme_index', {institutionId: institution.id}) }}">{{ "b"|trans }}</a>
    {{ form(form) }}
{% endblock %}
